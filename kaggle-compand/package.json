{
  "name": "kaggle-compand",
  "displayName": "Kaggle Compand",
  "description": "Kaggle competition management including features for file submission, score tracking, and dataset downloading.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": ["*"],
  "main": "./extension.js",
  "contributes": {
    "commands": [{
      "command": "kaggle-compand.helloWorld",
      "title": "Hello World"
    }, {
      "command": "kaggle-compand.kaggleLogin",
      "title": "Login"
    }, {
      "command": "kaggle-compand.submission",
      "title": "Submit"
    }, {
      "command": "kaggle-compand.competitions",
      "title": "Get Competitions"
    }, {
      "command": "kaggle-compand.showSidebar",
      "title": "Sidebar"
    }]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.92.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  }
}
